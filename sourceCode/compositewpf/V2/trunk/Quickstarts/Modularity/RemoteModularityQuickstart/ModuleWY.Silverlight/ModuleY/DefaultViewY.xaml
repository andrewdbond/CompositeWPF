<UserControl x:Class="ModuleY.DefaultViewY"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" >
    <Grid x:Name="LayoutRoot" Background="White">
        <Border Margin="10,10,10,10" CornerRadius="4,4,4,4" BorderBrush="#193441" Background="#FFFFFF" BorderThickness="2,2,2,2" VerticalAlignment="Center" HorizontalAlignment="Center" Height="60">
                <StackPanel>
                    <TextBlock VerticalAlignment="Center" HorizontalAlignment="Center">Module Y Loaded</TextBlock>
                    <Button  Padding="5" VerticalAlignment="Center" Width="95" Click="LoadModule_ButtonClick" Content="Load Module X"></Button>
            </StackPanel>
        </Border>
    </Grid>
</UserControl>
