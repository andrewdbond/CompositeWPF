<UserControl x:Class="ModuleB.DefaultViewB"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Border Margin="10,10,10,10" CornerRadius="4,4,4,4" 
    BorderBrush="#193441" Background="#FFFFFF" BorderThickness="2,2,2,2" VerticalAlignment="Center" HorizontalAlignment="Center" Height="60">
        <StackPanel>
            <Label VerticalAlignment="Center" HorizontalAlignment="Center">Module B Loaded</Label>
            <Button Style="{DynamicResource SimpleButton}" Padding="5" Click="OnLoadModuleCClick" VerticalAlignment="Center" HorizontalAlignment="Center">Load Module C</Button>
        </StackPanel>
    </Border>
</UserControl>
