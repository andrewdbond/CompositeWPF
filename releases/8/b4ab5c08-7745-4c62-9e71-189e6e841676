    <script type="text/javascript">
        var interval, count = 3;
        function startDownload() {
            interval = setInterval(function() {
                count -= 1;
                if (count == 0) {
                    clearInterval(interval);
                    
                    $('#CountdownMessage').html("Click <b>" +
                        '<a id="downloadLink"' +
                        'href="#"' + 
                        'd:postUrl="https://compositewpf.codeplex.com/releases/captureDownload"' +
                        'd:fileid="38150"' +
                        'd:clickoncepath=""' +
                        'd:redirectToAds="false"' +
                        'onclick="return $.release.fn.downloadFile(this);">' +
                        'here</a></b> if your browser does not start the download automatically.');
                        $.release.fn.downloadFile($('a#downloadLink'));
                    
                } else {
                  $('#Countdown').text(count + ' second' + (count != 1 ? 's' : ''));
                }
            }, 1000);
        }
    </script>
    <div id="ReleaseDownload">
        <h1 class="page_title">downloading...</h1>

        
            <p>
                <a href="https://compositewpf.codeplex.com/releases/view/14982">View the release</a> this file is associated with.
                <span id="CountdownMessage">Your download will start in <span id="Countdown">3 seconds</span>.</span>
            </p>
            <script type="text/javascript">
                $(document).ready(function() {
                    startDownload();
                });
            </script>
        
